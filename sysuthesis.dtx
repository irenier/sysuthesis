\hook_gput_code:nnn { package / algorithm2e / after } { . }
  {
%    \end{macrocode}
% 设置算法、算法目录名称。
%    \begin{macrocode}
    \SetAlgorithmName { \c_@@_name_algorithm_tl }
      { \c_@@_name_algorithm_tl } { \c_@@_name_loa_tl }
%    \end{macrocode}
% 设置算法目录。
