% \begin{macro}{\@@_vskip:N}
% 类似于 \LaTeXe 中的 \tn{vspace*}
% \footnote{\url{https://tex.stackexchange.com/a/30065/251992}}，
% 从上一个页面元素底部开始生成 |skip|。
%    \begin{macrocode}
\cs_new_protected:Npn \@@_vskip:N #1
  {
    \tex_hrule:D \@height \c_zero_dim \scan_stop:
    \tex_penalty:D \@M
    \skip_vertical:N #1
    \skip_vertical:N \c_zero_dim
  }
%    \end{macrocode}
