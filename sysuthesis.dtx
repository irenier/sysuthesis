%   \item bool 型变量，控制目录中是否有章标题
%   \item 章标题内容
%   \item 章标题格式
% \end{arguments}
%    \begin{macrocode}
\cs_new_protected:Npn \@@_chapter:Nnn #1#2#3
  {
    \chapter* { #3 #2 }
    \bool_if:NTF #1
      { \addcontentsline { toc } { chapter } { #2 } }
      { \pdfbookmark [ 0 ] { #2 } { \@currentHref } }
    \markboth { #2 } { }
  }
\cs_new_protected:Npn \@@_chapter:nn #1#2
  { \@@_chapter:Nnn \c_false_bool { #1 } { #2 } }
\cs_new_protected:Npn \@@_chapter:n #1
  { \@@_chapter:Nnn \c_false_bool { #1 } { } }
\cs_generate_variant:Nn \@@_chapter:nn { en }
%    \end{macrocode}
% \end{macro}
%
% \begin{macro}{\@@_bookmark:n}
% 添加书签。
%    \begin{macrocode}
\cs_new_protected:Npn \@@_bookmark:n #1
  {
    \phantomsection
    \pdfbookmark [ 0 ] { #1 } { \@currentHref }
    \markboth { #1 } { }
  }
%    \end{macrocode}
% \end{macro}
